{
  "dashboard": {
    "id": null,
    "title": "BMAD Enterprise Overview - Executive Dashboard",
    "description": "Executive-level overview of enterprise data platform health, performance, and business metrics",
    "tags": ["enterprise", "executive", "bmad", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "system_health_score",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "SLA Compliance",
        "type": "gauge",
        "targets": [
          {
            "expr": "sla_compliance_percent",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "min": 90,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 90},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Cost Efficiency Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "cost_per_transaction",
            "refId": "A",
            "legendFormat": "Cost per Transaction"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  },
  "folderId": 0,
  "overwrite": true
